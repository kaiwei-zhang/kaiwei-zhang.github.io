%% Theorem-type environment

\theoremstyle{plain}
\newtheorem{thm}{\bf Theorem}
\newtheorem*{thm*}{\bf Theorem}
\newtheorem{lem}[thm]{\bf Lemma}
\newtheorem*{lem*}{\bf Lemma}
\newtheorem{prop}[thm]{\bf Proposition}
\newtheorem*{prop*}{\bf Proposition}
\newtheorem{cor}[thm]{\bf Corollary}
\newtheorem*{cor*}{\bf Corollary}
\newtheorem{asmp}{\bf Assumption}
\newtheorem*{claim}{Claim}

\theoremstyle{definition}
\newtheorem{defn}[thm]{\bf Definition}
\newtheorem*{defn*}{\bf Definition}
\newtheorem{conj}[thm]{\bf Conjecture}
\newtheorem{exmp}{\bf Example}
\newtheorem{prob}{}
\newtheorem{subprob}{}[section]
\renewcommand{\thesubprob}{(\alph{subprob})}
\newtheorem{algorithm}{\bf Algorithm}
\newtheorem*{algorithm*}{\bf Algorithm}

\theoremstyle{remark}
\newtheorem*{rem}{\bf Remark}
%\newtheorem*{note}{\bf Note}

% Cases and steps, to be used in proofs.
% If used in text, put ``\setcounter{case}{0}" before.
\newtheoremstyle{case}{}{}{}{}{\bfseries\boldmath}{.}{.5em}
{\thmname{#1} \thmnumber{#2}: \thmnote{#3}}
\theoremstyle{case}
\newtheorem{case}{\bf Case}[thm]
\renewcommand{\thecase}{\arabic{case}}
\newtheorem{subcase}{\bf Case}[case]

\newtheoremstyle{step}{}{}{\itshape}{}{\itshape}{.}{.5em}{\thmname{#1} \thmnumber{#2}}
\theoremstyle{step}
\newtheorem{step}{Step}[thm]
\renewcommand{\thestep}{\arabic{step}}

% one-shot theorem-like environment such as ``Zorn's Lemma"
\newtheoremstyle{oneshot}{}{}{}{}{\bfseries\boldmath}{.}{.5em}{\thmnote{#3}}
\theoremstyle{oneshot}
\newtheorem*{oneshot}{}

